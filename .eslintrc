{
  "env": {
    "es2021": true,
    "mocha": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "global-require": "off",
    "import/no-dynamic-require": "off",
    "no-param-reassign": "off",
    "object-curly-newline": "off"
  }
}
